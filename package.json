{
  "name": "mock-pl",
  "version": "0.1.0",
  "description": "A simple API that serves the latest scores of fixtures of matches in a “Mock Premier League”",
  "main": "index.js",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "start": "node dist/",
    "start:run": "yarn build && yarn start",
    "dev": "nodemon --watch 'src/*' --exec 'yarn build && node ./dist/index'",
    "lint": "./node_modules/.bin/eslint . --fix",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --runInBand --detectOpenHandles --coverage --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --testTimeout=10000 --runInBand --detectOpenHandles --watch",
    "pretest": "cross-env NODE_ENV=test echo \\\"Pretesting and Deleting DB Record\\\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jattoabdul/mock-pl.git"
  },
  "keywords": [
    "Node",
    "Express",
    "Schema",
    "Validation",
    "API",
    "data",
    "validation",
    "Premier League",
    "Live Scores"
  ],
  "author": "Abdulqahhar Aminujatto <jattoade@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jattoabdul/mock-pl/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "homepage": "https://github.com/jattoabdul/mock-pl#readme",
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "D": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.13",
    "morgan": "~1.9.1",
    "redis": "^3.0.2",
    "regenerator-runtime": "^0.13.5",
    "templatestringparser": "^1.1.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-stage-3": "^7.0.0",
    "@shelf/jest-mongodb": "^1.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.1",
    "redis-mock": "^0.49.0",
    "supertest": "^4.0.2",
    "yarn": "^1.19.1"
  }
}
